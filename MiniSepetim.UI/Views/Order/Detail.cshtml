@model MiniSepetim.Entities.Concrete.OrderDetail

@{
    Layout = null;
}



<div>
    <h4>Sipariş Detayı</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Satıcı
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Product.Seller.Name)
        </dd>
        <dt class="col-sm-2">
            Ürün Adı
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Product.Name)
        </dd>
        <dt class="col-sm-2">
            Fiyatı
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Product.Price)
        </dd>
        <dt class="col-sm-2">
            Oluşturulma Tarihi
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.CreateDate)
        </dd>
        <dt class="col-sm-2">
            Durumu
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.State)
        </dd>
    </dl>
</div>
